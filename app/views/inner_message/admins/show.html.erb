<div class="jumbotron">
  <div class="container text-center">
    <h1>Channels</h1>
  </div>
</div>
<div class="container">
<div class="row">
  <div class="col-md-9">
    <div class="panel panel-default">
      <div class="panel-heading" data-bind="click: $root.returnToChannelList">Channels</div>
      <div class="panel-body">
        <%= render 'channel_list' %>
        <%= render 'channel_detail' %>
        <%= render 'channel_new_form' %>
      </div>
    </div>
  </div>
  <div class="col-md-2">
    <div class="form-group">
      <button class="btn btn-success btn-block" data-bind="click: newChannel">
        <i class="fa fa-plus"></i>
        Add Channel
      </button>
    </div>
  </div>
</div>
</div>